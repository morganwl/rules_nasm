load("@bazel_skylib//lib:selects.bzl", "selects")

package(default_visibility = ["//visibility:public"])

filegroup(name = "empty")

selects.config_setting_group(
    name = "elf",
    match_any = [
        "@platforms//os:linux",
    ],
)

platform(
    name = "elf64",
    constraint_values = [
        ":elf",
        "@platforms//cpu:x86_64",
    ]
)
